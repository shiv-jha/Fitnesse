		&lt;td&gt;numerator&lt;/td&gt;
		&lt;td&gt;&lt;/td&gt;
		&lt;td&gt;denominator&lt;/td&gt;
		&lt;td&gt;&lt;/td&gt;
		&lt;td&gt;quotient?&lt;/td&gt;
	&lt;/tr&gt;
	&lt;tr class="slimRowColor9"&gt;
		&lt;td&gt;setNumerator&lt;/td&gt;
		&lt;td colspan="6"&gt;35&lt;/td&gt;
	&lt;/tr&gt;
	&lt;tr class="slimRowColor4"&gt;
		&lt;td&gt;setDenominator&lt;/td&gt;
		&lt;td colspan="6"&gt;5&lt;/td&gt;
	&lt;/tr&gt;
	&lt;tr class="slimRowColor6"&gt;
		&lt;td&gt;$quotient&amp;lt;-[7.0]&lt;/td&gt;
